<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

<bean id="dateFormat" class="java.text.SimpleDateFormat">
<constructor-arg value="yyyy-MM-dd"></constructor-arg>
</bean>


<bean class="com.xworkz.dto.RocketDTO" id="rocketDTO">
<property name="country" value="India"></property>
<property name="launchDate" >
<bean factory-bean="dateFormat" factory-method="parse">
<constructor-arg value="2020-11-07"></constructor-arg></bean> 
</property>

<property name="cost" value="325"></property>
</bean>

<bean class="com.xworkz.dao.RocketDAO" id="rocketDAO" init-method="init" destroy-method="destroy">
<property name="userName" value="root"></property>
<property name="password" value="root"></property>
<property name="url" value="jdbc:mysql://localhost:3306/"></property>
<property name="driverClass" value="com.mysql.cj.jdbc.Driver"></property>
</bean>



</beans>